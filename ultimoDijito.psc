Algoritmo ultimoDijito 
	definir n1, n2 , n3,d1, d3 como entero;
	escribir "Ingresar valor de n1";
	leer n1;
	escribir "Ingresar valor de n2";
	leer n2;
	escribir"ingresar el valor de n3 ";
	leer n3 ;
	d1=n3%10;
     escribir d1;
	d2=n1%10;
	escribir d2;
	d3=n2%10;
	escribir d3;
	Si d1==d2 Y d1==d3 Entonces
		escribir "los dijitos son iguales";
	SiNo
	   escribir "los djitos son diferentes";
	Fin Si
	

FinAlgoritmo
