Algoritmo VolumenCilindro
	definir radio como entero;
	definir altura como entero;
	definir volumen como real;
	escribir "Ingresa el valor del radio ";
	leer radio;
	escribir"ingresa el valor de la altura ";
	leer altura;
	volumen= radio*radio*pi*altura;
	escribir" elvolumen del cilindro es :"    volumen ; 
FinAlgoritmo
