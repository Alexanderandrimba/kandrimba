Algoritmo LICENCIAS
		DEFINIR L1 COMO CARACTER  ;
		ESCRIBIR "ESCRIBA SU TIPO DE LICENCIA ";
		LEER L1 ;
		Si L1=="A" Entonces 
			Escribir "USTED PUEDE MANEJAR MOTOS ";
		FinSi;
		 Si L1=="B" Entonces 
				Escribir "USTED PUEDE MANEJAR VEICULO PARTICULAR  ";
			FinSi;
			Si L1=="C" Entonces 
				Escribir "USTED PUEDE MANEJAR VEICULOS PESADOS ";
			FinSi;
			Si L1=="D" Entonces 
				Escribir "USTED PUEDE MANEJAR VEICULOS ESPECIALES";
			FinSi;
		
		

FinAlgoritmo
